import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T = sc.nextInt();
		while(T-->0){
		    int X = sc.nextInt();
		    int Y = sc.nextInt();
		    int Z = sc.nextInt();
		    
		    int timeToHit = Y/X;
		    int waitTime = Z - timeToHit;
		    
		    if(waitTime < 0){
		        System.out.println(0);
		    } else {
		        System.out.println(waitTime);
		    }
		}
	}
}
