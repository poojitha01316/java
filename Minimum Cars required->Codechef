import java.util.*;
import java.lang.*;

class Codechef {
    public static void main(String[] args) throws java.lang.Exception {
        Scanner sc = new Scanner(System.in); 

        int t = sc.nextInt();

        while (t-- > 0) { 
            int n = sc.nextInt();
            int cars = (n + 3) / 4;

            System.out.println(cars);
        }
        sc.close();
    }
}
